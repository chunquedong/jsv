<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<servlet>
		<servlet-name>RouteServlet</servlet-name>
		<servlet-class>chunquedong.jsv.route.RouteServlet</servlet-class>
		<init-param>
			<param-name>actionPackage</param-name>
			<param-value>chunquedong.jsv.demo.action</param-value>
		</init-param>
		<init-param>
			<param-name>debugMode</param-name>
			<param-value>debug</param-value>
		</init-param>
		<init-param>
			<param-name>profix</param-name>
			<param-value>/test</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>RouteServlet</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
</web-app>
